<?xml version="1.0"?>
<extension key="biz.jmaconsulting.currentpaymentversion" type="module">
  <file>currentpaymentversion</file>
  <name>Show current version of payments</name>
  <description>Show only the current state of each payment transaction. Hide payment reversals and previous versions of each payment transaction.</description>
  <license>AGPL-3.0</license>
  <maintainer>
    <author>Monish Deb</author>
    <email>everyone@jmaconsulting.biz</email>
  </maintainer>
  <urls>
    <url desc="Main Extension Page">https://github.com/JMAConsulting/biz.jmaconsulting.currentpaymentversion</url>
    <url desc="Documentation">https://github.com/JMAConsulting/biz.jmaconsulting.currentpaymentversion/blob/master/README.md</url>
    <url desc="Support">https://github.com/JMAConsulting/biz.jmaconsulting.currentpaymentversion/issues</url>
    <url desc="Licensing">http://www.gnu.org/licenses/agpl-3.0.html</url>
  </urls>
  <releaseDate>2018-10-17</releaseDate>
  <version>1.0</version>
  <develStage>stable</develStage>
  <compatibility>
    <ver>5.7</ver>
  </compatibility>
  <comments>Show only the current state of each payment transaction. Hide payment reversals and previous versions of each payment transaction.</comments>
  <civix>
    <namespace>CRM/CurrentPaymentVersion</namespace>
  </civix>
</extension>
